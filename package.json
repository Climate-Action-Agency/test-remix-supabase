{
  "name": "test-remix-supabase",
  "description": "Test app for Remix and Supabase",
  "version": "0.1.0",
  "main": "remix.config.js",
  "license": "ISC",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "test": "npx supabase test db",
    "gentypes": "cd ./supabase; npx --yes supabase gen types --lang=typescript --local --schema public,basejump > ../app/types/supabase/index.d.ts; npx eslint --fix ../app/types/supabase/index.d.ts",
    "dev": "remix vite:dev",
    "build": "npm run lint && remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "clean": "rm -rf build; rm -rf public/build; echo Cleaned build directories\n",
    "lint": "npm run clean && eslint .",
    "fix": "npm run clean && eslint --fix .",
    "precommit": "npm run fix && npx supabase db reset && npm run test && npm run gentypes && npm run fix",
    "typecheck": "tsc"
  },
  "engines": {
    "node": ">=20.0.0"
  },
